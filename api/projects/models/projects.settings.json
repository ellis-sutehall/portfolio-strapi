{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Project"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "body": {
      "type": "richtext"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "stacks": {
      "collection": "stack"
    },
    "github_link": {
      "type": "string"
    },
    "project_link": {
      "type": "string"
    },
    "summary": {
      "type": "text"
    },
    "thumbnail": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "Order": {
      "type": "integer"
    },
    "meta_description": {
      "type": "string"
    }
  }
}
